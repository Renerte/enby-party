{
  "name": "enby-party",
  "version": "1.0.0",
  "author": "Renn <contact@renerte.net>",
  "private": true,
  "scripts": {
    "build": "yarn clean && parcel build src/index.html --no-source-maps",
    "clean": "concurrently \"yarn clean:cache\" \"yarn clean:dist\"",
    "clean:cache": "rm -rf .parcel-cache",
    "clean:dist": "rm -rf dist",
    "lint": "eslint src",
    "start": "parcel src/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@parcel/transformer-sass": "^2.3.1",
    "@parcel/transformer-vue": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^8.4.1",
    "parcel": "^2.3.1",
    "postcss": "^8.4.6",
    "pug": "^3.0.2",
    "sass": "^1.49.7",
    "typescript": "^4.5.5",
    "vue": "^3.2.31"
  },
  "packageManager": "yarn@3.1.1"
}
