{
  "name": "enby-party",
  "version": "1.0.0",
  "author": "Renn <contact@renerte.net>",
  "private": true,
  "scripts": {
    "build": "yarn clean && parcel build src/index.html --no-source-maps",
    "clean": "concurrently \"yarn clean:cache\" \"yarn clean:dist\"",
    "clean:cache": "rm -rf .parcel-cache",
    "clean:dist": "rm -rf dist",
    "lint": "eslint src",
    "start": "parcel src/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@parcel/transformer-sass": "2.0.0-beta.3.1",
    "@parcel/transformer-vue": "2.0.0-beta.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.17.0",
    "parcel": "next",
    "postcss": "^8.3.6",
    "pug": "^3.0.2",
    "sass": "^1.39.2",
    "typescript": "^4.4.3",
    "vue": "^3.2.11"
  }
}
