{
  "name": "enby-party",
  "version": "1.0.0",
  "author": "Renn <contact@renerte.net>",
  "private": true,
  "scripts": {
    "build": "bun run clean:dist && parcel build src/index.html --no-source-maps",
    "clean": "concurrently \"bun run clean:cache\" \"bun run clean:dist\"",
    "clean:cache": "rm -rf .parcel-cache",
    "clean:dist": "rm -rf dist",
    "lint": "biome lint src",
    "start": "parcel src/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@biomejs/biome": "^2.2.2",
    "@parcel/core": "^2.15.4",
    "@parcel/transformer-sass": "^2.15.4",
    "@parcel/transformer-vue": "^2.15.4",
    "@vue/language-plugin-pug": "^3.0.6",
    "concurrently": "^9.2.1",
    "globals": "^16.3.0",
    "parcel": "^2.15.4",
    "postcss": "^8.5.6",
    "pug": "^3.0.3",
    "sass": "^1.91.0",
    "typescript": "^5.9.2",
    "vue": "^3.5.20"
  },
  "volta": {
    "node": "22.19.0"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@swc/core",
    "lmdb",
    "msgpackr-extract"
  ]
}
